<svg width="512" height="512" viewBox="0 0 512 512" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <pattern id="texture" patternUnits="userSpaceOnUse" width="8" height="8">
      <rect width="8" height="8" fill="#2E7D32"/>
      <circle cx="4" cy="4" r="1" fill="#1B5E20" opacity="0.3"/>
    </pattern>
  </defs>
  
  <!-- Background -->
  <rect width="512" height="512" fill="url(#texture)" rx="64"/>
  
  <!-- Speech bubble -->
  <g transform="translate(128, 80)">
    <!-- Main bubble -->
    <path d="M 50 50 L 200 50 Q 250 50 250 100 L 250 200 Q 250 250 200 250 L 50 250 Q 0 250 0 200 L 0 100 Q 0 50 50 50 Z" 
          fill="white" stroke="none"/>
    
    <!-- Tail -->
    <path d="M 50 250 L 80 300 L 110 250 Z" fill="white"/>
    
    <!-- Leaf icon -->
    <g transform="translate(125, 90)">
      <path d="M 0 -20 Q -15 -30 -20 -15 Q -15 0 -5 10 Q 5 20 15 10 Q 20 0 15 -15 Q 10 -30 0 -20 Z" 
            fill="white" stroke="none"/>
      <line x1="0" y1="-20" x2="0" y2="10" stroke="white" stroke-width="3"/>
      <path d="M -5 0 Q 0 -5 5 0" stroke="white" stroke-width="2" fill="none"/>
    </g>
    
    <!-- ECO text -->
    <text x="125" y="150" text-anchor="middle" font-family="Arial, sans-serif" font-size="30" font-weight="bold" fill="white">ECO</text>
    
    <!-- QUIZ text -->
    <text x="125" y="190" text-anchor="middle" font-family="Arial, sans-serif" font-size="30" font-weight="bold" fill="white">QUIZ</text>
  </g>
</svg>
